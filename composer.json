{
    "name": "marcel-maqsood/session-auth",
    "description": "The SessionAuthMiddleware is a PSR-15 middleware that provides handling for sessions and logins in a Laminas/Mezzio application",
    "keywords": [
        "framework",
        "MazeDEV",
        "session",
        "authentication",
        "middleware"
    ],
    "license": "MIT",
    "homepage": "https://github.com/marcel-maqsood/Mezzio-Session-Auth-Middleware",
    "authors": [
        {
            "name": "Technikhafen",
            "homepage": "https://technikhafen.de"
        },
        {
            "name": "Marcel Maqsood"
        }
    ],
    "autoload": {
        "psr-4": {
            "MazeDEV\\SessionAuth\\": "src/"
        }
    },
    "extra": {
        "laminas": {
            "component": "MazeDEV\\SessionAuth",
            "config-provider": "MazeDEV\\SessionAuth\\ConfigProvider"
        }
    },
    "require": {
        "mezzio/mezzio-authentication": "^1.9",
        "mezzio/mezzio-authentication-session": "^1.9",
        "webmozart/assert": "^1.11",
        "psr/container": "^1.0",
        "mezzio/mezzio-router": "^3.17",
        "marcel-maqsood/database-connector": "^1.0",
        "laminas/laminas-diactoros": "^3.3",
        "marcel-maqsood/mezzio-abstract-request-handler": "^0.1.0",
        "mezzio/mezzio-helpers": "^5.16",
        "mezzio/mezzio-template": "^2.10",
        "mezzio/mezzio-session-ext": "^1.20"
    }
}
